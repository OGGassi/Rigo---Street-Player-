    {
      "name": "verse1",
      "type": "verse",
      "lyrics": [
        {
          "line": "הלב שלי עוד פועם בקצב משלו",
          "chords": [
            {"chord": "G", "position": 0},
            {"chord": "D/F#", "position": 11},
            {"chord": "Em", "position": 20}
          ]
        },
        {
          "line": "וכל הדרכים עוד פתוחות לפניי",
          "chords": [
            {"chord": "C", "position": 0},
            {"chord": "D", "position": 13},
            {"chord": "G", "position": 26}
          ]
        },
        {
          "line": "אני צועד לאט, לאט",
          "chords": [
            {"chord": "Em", "position": 0},
            {"chord": "C", "position": 12}
          ]
        },
        {
          "line": "באור ובחושך מוצא את דרכי",
          "chords": [
            {"chord": "G", "position": 0},
            {"chord": "D", "position": 15},
            {"chord": "C", "position": 23}
          ]
        }
      ]
    },
    {
      "name": "pre_chorus",
      "type": "pre_chorus",
      "lyrics": [
        {
          "line": "אני לא עוצר, לא נותן לזמן",
          "chords": [
            {"chord": "Am", "position": 0},
            {"chord": "D", "position": 20}
          ]
        },
        {
          "line": "לקחת ממני את הרגע הזה",
          "chords": [
            {"chord": "G", "position": 0},
            {"chord": "Em", "position": 12}
          ]
        },
        {
          "line": "כי עוד לא אבדה, העיר הגדולה",
          "chords": [
            {"chord": "C", "position": 0},
            {"chord": "D", "position": 20}
          ]
        }
      ]
    },
    {
      "name": "chorus",
      "type": "chorus",
      "lyrics": [
        {
          "line": "הלב שלי, עוד שומר אמונים",
          "chords": [
            {"chord": "G", "position": 0},
            {"chord": "D/F#", "position": 9},
            {"chord": "Em", "position": 21}
          ]
        },
        {
          "line": "לכל אהבותיי וחלומותיי",
          "chords": [
            {"chord": "C", "position": 0},
            {"chord": "D", "position": 18}
          ]
        },
        {
          "line": "הלב שלי, עוד לא נואש לגמרי",
          "chords": [
            {"chord": "G", "position": 0},
            {"chord": "D/F#", "position": 9},
            {"chord": "Em", "position": 23}
          ]
        },
        {
          "line": "עוד ידע אושר וימים טובים",
          "chords": [
            {"chord": "C", "position": 0},
            {"chord": "D", "position": 11},
            {"chord": "G", "position": 21}
          ]
        }
      ]
    },
    {
      "name": "verse2",
      "type": "verse",
      "lyrics": [
        {
          "line": "הלב שלי כמו ילד שיודע",
          "chords": [
            {"chord": "G", "position": 0},
            {"chord": "D/F#", "position": 11},
            {"chord": "Em", "position": 19}
          ]
        },
        {
          "line": "שאחרי לילה תמיד עולה שמש",
          "chords": [
            {"chord": "C", "position": 0},
            {"chord": "D", "position": 14},
            {"chord": "G", "position": 24}
          ]
        },
        {
          "line": "אני רואה רחוק, רחוק",
          "chords": [
            {"chord": "Em", "position": 0},
            {"chord": "C", "position": 15}
          ]
        },
        {
          "line": "מעבר לאופק שמחר יביא",
          "chords": [
            {"chord": "G", "position": 0},
            {"chord": "D", "position": 13},
            {"chord": "C", "position": 21}
          ]
        }
      ]
    },
